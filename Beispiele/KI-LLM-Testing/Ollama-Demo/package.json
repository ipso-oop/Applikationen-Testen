{
  "name": "ollama-demo",
  "version": "1.0.0",
  "description": "Comprehensive evaluation system for Ollama Mistral model with Langfuse tracing",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "ts-node --watch index.ts",
    "build": "tsc",
    "test": "ts-node test-all.ts",
    "test:simple": "ts-node simple-test.ts",
    "test:performance": "ts-node performance-test-simple.ts",
    "test:accuracy": "ts-node accuracy-test-simple.ts",
    "test:creative": "ts-node creative-test-simple.ts",
    "test:coding": "ts-node coding-test-simple.ts",
    "evaluate": "ts-node ollama-evaluation.ts",
    "performance": "ts-node performance-test.ts",
    "accuracy": "ts-node accuracy-test.ts",
    "creative": "ts-node creative-test.ts",
    "coding": "ts-node coding-test.ts",
    "all": "npm run evaluate && npm run performance && npm run accuracy && npm run creative && npm run coding"
  },
  "dependencies": {
    "@langfuse/otel": "^4.0.1",
    "@langfuse/tracing": "^4.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "axios": "^1.13.5",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.2",
    "langfuse": "^2.8.0",
    "ora": "^5.4.1",
    "source-map-support": "^0.5.21",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
